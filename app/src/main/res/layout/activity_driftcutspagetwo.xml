<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="driftcutspagetwoVM" type="com.collagepoem.app.modules.driftcutspagetwo.data.viewmodel.DriftcutspagetwoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackback"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_200">
        <LinearLayout android:id="@+id/linearColumnback"
            android:layout_gravity="top|end"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_73pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowback"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageBack"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_imageback"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_imageback"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTextholder"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_138pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtTextholder}"
                        style="@style/txtArialmt14_1" tools:text="@string/lbl_flow_away"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageStickwithbg"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_99pxh"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_59pxv"
                android:layout_width="@dimen/_312pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_stickwithbg}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_stickwithbg"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtextfrom"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTextfrom"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.32"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtTextfrom}"
                    style="@style/txtArialmt14_1" tools:text="@string/lbl_from"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTexttime"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_165pxh"
                    android:layout_weight="0.68"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtTexttime}"
                    style="@style/txtArialmt14_1" tools:text="@string/lbl_collect_time"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowbridcollectta"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearImgfrom"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.52"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylegray_800">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnbridcollectta"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylegray_700">
                        <TextView android:gravity="start"
                            android:id="@+id/txtBridcollectTa"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_70pxv"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_32pxv"
                            android:layout_width="match_parent"
                            android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtBridcollectTa}"
                            style="@style/txtSTZhongsong14_1" tools:text="@string/msg_brid_collect_ta"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearImgtime"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="65dp"
                    android:layout_weight="0.48"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_A700">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtDuration"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_70pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="match_parent"
                        android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtDuration}"
                        style="@style/txtArialmt14_1" tools:text="@string/msg_2022_10_12_1day"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageInside"
                android:layout_gravity="end"
                android:layout_height="@dimen/_47pxh"
                android:layout_marginEnd="@dimen/_112pxh"
                android:layout_marginStart="@dimen/_112pxh"
                android:layout_marginTop="@dimen/_85pxv"
                android:layout_width="@dimen/_40pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_inside"
                tools:ignore="ContentDescription" tools:src="@drawable/img_inside"/>
            <FrameLayout android:id="@+id/frameStackdurationOne"
                android:layout_gravity="end"
                android:layout_height="@dimen/_166pxv"
                android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_357pxh">
                <TextView android:gravity="top|start"
                    android:id="@+id/txtDurationOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{driftcutspagetwoVM.driftcutspagetwoModel.txtDurationOne}"
                    style="@style/txtSTZhongsong21" tools:text="@string/msg_left1daystick_w"/>
                <ImageView android:id="@+id/imageBottonTwo"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_166pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_129pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_botton2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_botton2"/>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
