<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="camerapageVM" type="com.collagepoem.app.modules.camerapage.data.viewmodel.CamerapageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCamerapage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_200">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_background_4"
            tools:ignore="ContentDescription" tools:src="@drawable/img_background_4"/>
        <LinearLayout android:id="@+id/linearColumnbackbtn"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageBackbtn"
                android:layout_gravity="start"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_backbtn"
                tools:ignore="ContentDescription" tools:src="@drawable/img_backbtn"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackcover"
                android:layout_height="@dimen/_229pxv"
                android:layout_marginTop="@dimen/_595pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageCover"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_229pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_cover}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_cover"/>
                <LinearLayout android:id="@+id/linearRowalbum"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_49pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_49pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageAlbum"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="@dimen/_44pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_album"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_album"/>
                    <ImageView android:id="@+id/imageShot"
                        android:layout_height="@dimen/_74pxh"
                        android:layout_marginStart="@dimen/_104pxh"
                        android:layout_width="@dimen/_74pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_shot}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shot"/>
                    <ImageView android:id="@+id/imageShift"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_112pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_shift"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_shift"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
